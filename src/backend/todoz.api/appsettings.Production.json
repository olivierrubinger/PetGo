{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Error",
            "Microsoft.Hosting.Lifetime": "Information"
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "File": {
            "LogLevel": {
                "Default": "Warning"
            },
            "Path": "Logs/app.log",
            "RollingInterval": "Day"
        }
    },
    "AllowedHosts": "*"
}